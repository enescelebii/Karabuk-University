
public class Teacher {
	int i;
	Branch ders;
	Teacher(int i){
		int id;
		switch(i) {
		case 1:
			id = 1;
			ders = Branch.ENG;
			break;
		case 2:
			id = 2;
			ders = Branch.CS;
		case 3:
			id = 3;
			ders = Branch.MATH;
			break;
		case 4:
			id = 4;
			ders = Branch.PHYSICS;
		}
	}
	public static Teacher[] values() {
		return ders;
	}
}
